{
  "name": "amnesty",
  "private": true,
  "scripts": {
    "dev": "yarn install --frozen-lockfile && concurrently --kill-others-on-fail --names app,_db --prefix-colors blue.inverse,green 'yarn:dev:start:app' 'yarn:dev:start:db'",
    "dev:start:db": "docker compose up",
    "dev:start:app": "wait-on tcp:5432 --interval 5000 && yarn db:migrate:dev && next dev",
    "dev:start:studio": "prisma studio",
    "build": "next build",
    "vercel-build": "yarn db:generate:prod && yarn db:migrate:prod && next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate:dev": "prisma generate && prisma migrate dev",
    "db:generate:prod": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate",
    "db:migrate:prod": " DATABASE_URL=\"$DIRECT_DATABASE_URL\" ./prod-migrate.sh",
    "db:seed": "prisma db seed"
  },
  "dependencies": {
    "@emotion/react": "11.6.0",
    "@emotion/styled": "11.6.0",
    "@mui/icons-material": "5.2.0",
    "@mui/material": "5.2.1",
    "@mui/system": "5.2.1",
    "@mui/x-data-grid": "5.0.1",
    "@prisma/client": "3.5.0",
    "axios": "0.24.0",
    "bcrypt": "5.0.1",
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.20.3",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/node": "16.11.10",
    "@types/react": "17.0.37",
    "babel-plugin-superjson-next": "^0.4.2",
    "concurrently": "6.4.0",
    "dotenv": "10.0.0",
    "eslint": "7",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.5.0",
    "prisma": "3.5.0",
    "superjson": "^1.8.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.2",
    "wait-on": "6.0.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
